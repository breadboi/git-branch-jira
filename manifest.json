{
    "manifest_version": 3,
    "name": "Jira Git Branch Helper",
    "version": "1.0",
    "description": "Copy Jira issue number as git branch name",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://*.atlassian.net/browse/*", "https://*.atlassian.net/jira/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  